{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
    	"include": [ "js", "README.md" ],
    	"exclude": ["js/dojo", "js/dat.gui.js", "js/dat.gui.min.js", "js/dat.gui.removeFolder.js", "js/html-imports.min.js", "js/require.js", "js/three.js", "js/three.min.js", "js/OBJLoader.js", "js/OrbitControls.js", "js/STLLoader.js"], 
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./docs/jsdoc/"
    }
}
